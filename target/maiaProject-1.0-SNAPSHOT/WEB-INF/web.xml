<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <filter>
        <filter-name>ResponseOverrideFilter</filter-name>
        <filter-class>org.displaytag.filter.ResponseOverrideFilter</filter-class>
    </filter>
    <servlet>
        <servlet-name>ListaAluno</servlet-name>
        <servlet-class>Servlet.ListaAluno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletaAluno</servlet-name>
        <servlet-class>Servlet.DeletaAluno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarregaAluno</servlet-name>
        <servlet-class>Servlet.CarregaAluno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AtualizaAluno</servlet-name>
        <servlet-class>Servlet.AtualizaAluno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroAluno</servlet-name>
        <servlet-class>Servlet.CadastroAluno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroAdm</servlet-name>
        <servlet-class>Servlet.CadastroAdm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdm</servlet-name>
        <servlet-class>Servlet.LoginAdm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaAdmin</servlet-name>
        <servlet-class>Servlet.ListaAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroAluno</servlet-name>
        <url-pattern>/CadastroAluno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroAdm</servlet-name>
        <url-pattern>/CadastroAdm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaAluno</servlet-name>
        <url-pattern>/ListaAluno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletaAluno</servlet-name>
        <url-pattern>/DeletaAluno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarregaAluno</servlet-name>
        <url-pattern>/CarregaAluno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AtualizaAluno</servlet-name>
        <url-pattern>/AtualizaAluno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdm</servlet-name>
        <url-pattern>/LoginAdm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    <servlet-name>ListaAdmin</servlet-name>
        <url-pattern>/ListaAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <jsp-config>
        <taglib>
            <taglib-uri>http://displaytag.sf.net</taglib-uri>
            <taglib-location>/WEB-INF/displaytag-1.2.tld</taglib-location>
        </taglib>
    </jsp-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
